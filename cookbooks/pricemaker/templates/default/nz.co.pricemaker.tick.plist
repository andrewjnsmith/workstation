<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>nz.co.pricemaker.tick</string>
    <key>OnDemand</key>
    <true/>
    <key>ProgramArguments</key>
    <array>
    	<string><%= node["pmk_app"]["document_root"] %>/bin/phake</string>
    	<string>tick:all</string>
    </array>
    <key>WorkingDirectory</key>
    <string><%= node["pmk_app"]["document_root"] %>/bin</string>
    <key>StartInterval</key>
    <integer>60</integer>
</dict>
</plist>